$('.modifyLink').bind('ajax:success', function() {
   $(this).siblings('.note-text').contents().wrapAll("<textarea>").parent().unwrap();
   $(this).siblings('textarea').attr("id", "note_text")
   $(this).siblings('textarea').attr("name", "note_text")
   $(this).siblings('.deleteLink').addClass('d-none');
   $(this).siblings('.confirmButton').removeClass('d-none');
   $(this).addClass('d-none');
}); 